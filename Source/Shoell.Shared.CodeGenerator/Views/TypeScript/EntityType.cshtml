@model List<EntityType>
@{
    Layout = null;
    var namespaces = Model.GroupBy(e => e.Namespace);
}
export class EntityType {
    // Identity
    static Group = 'Group';
    static Role = 'Role';
    static User = 'User';
    static Role_Group = 'Role_Group';
    static User_Group = 'User_Group';

    // System
    static Log = 'Logs';
@foreach (var ns in namespaces)
{
<text>
    // @(ns.Key)
</text>
    @foreach (var type in ns.OrderBy(e => e.Name))
    {
<text>    static @(type.Name.AsPascaleCase()) = '@(type.Name.AsPascaleCase())';
</text>
    }
}
}
